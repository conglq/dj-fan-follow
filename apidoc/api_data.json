[  {    "type": "post",    "url": "/user/block",    "title": "Block an user",    "name": "Block_User",    "group": "DJ_FAN",    "description": "<p>Block User</p>",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "blockedUserId",            "description": "<p>id of blocked user</p>"          },          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "actionUserId",            "description": "<p>id of action user</p>"          }        ]      },      "examples": [        {          "title": "Input",          "content": "{\n  \"blockedUserId\": \"blockedUserId\"\n  \"actionUserId\": \"actionUserId\"\n}",          "type": "json"        }      ]    },    "success": {      "examples": [        {          "title": "Success-Response:",          "content": " HTTP/1.1 200 OK\n{\n  \"success\": true\n}",          "type": "json"        }      ]    },    "version": "0.0.0",    "filename": "./app.js",    "groupTitle": "DJ_FAN"  },  {    "type": "post",    "url": "/dj/follow",    "title": "Follow an user",    "name": "Follow_User",    "group": "DJ_FAN",    "description": "<p>Follow User</p>",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "djUserId",            "description": "<p>user id of DJ</p>"          },          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "actionUserId",            "description": "<p>user id of listener</p>"          }        ]      },      "examples": [        {          "title": "Input",          "content": "{\n  \"djUserId\": \"djUserId\"\n  \"actionUserId\": \"actionUserId\"\n}",          "type": "json"        }      ]    },    "success": {      "examples": [        {          "title": "Success-Response:",          "content": " HTTP/1.1 200 OK\n{\n  \"success\": true\n  \"fanCount\": 10\n}",          "type": "json"        }      ]    },    "version": "0.0.0",    "filename": "./app.js",    "groupTitle": "DJ_FAN"  },  {    "type": "get",    "url": "/dj/follow",    "title": "Get Fan List",    "name": "Get_Fan_List",    "group": "DJ_FAN",    "description": "<p>Get Fan List</p>",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "userId",            "description": "<p>user id of DJ</p>"          },          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "index",            "description": "<p>index value from response. 0 for default</p>"          },          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "nexti",            "description": "<p>nexti value from response, 0 for default</p>"          },          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "pagesize",            "description": "<p>number of item in page</p>"          }        ]      }    },    "examples": [      {        "title": "Example usage:",        "content": "curl - i http://localhost:9999/dj/follow?userId=djuserid0&index=0&nexti=0&pagesize=10",        "type": "curl"      }    ],    "success": {      "examples": [        {          "title": "Success-Response:",          "content": " HTTP/1.1 200 OK\n{\n  \"success\": true\n  \"index\": 1\n  \"nexti\": 10\n  \"fanList\": [\n    {\n      \"id\": \"id\",\n      \"name\": \"Mac\"\n    }\n  ]\n}",          "type": "json"        }      ]    },    "version": "0.0.0",    "filename": "./app.js",    "groupTitle": "DJ_FAN"  },  {    "type": "post",    "url": "/user/profile",    "title": "Get user profile",    "name": "Get_User_Profile",    "group": "DJ_FAN",    "description": "<p>Get User Profile</p>",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "userId",            "description": "<p>id of user profile</p>"          },          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "actionUserId",            "description": "<p>id of action user</p>"          }        ]      }    },    "examples": [      {        "title": "Example usage:",        "content": "curl - i http://localhost:9999/user/profile?userId=djuserid0&actionUserId=unblockUserId",        "type": "curl"      }    ],    "success": {      "examples": [        {          "title": "Success-Response:",          "content": " HTTP/1.1 200 OK\n{\n  \"success\": true,\n  \"profile\": {\n    \"fanCount\": 1\n  }\n}",          "type": "json"        }      ]    },    "version": "0.0.0",    "filename": "./app.js",    "groupTitle": "DJ_FAN"  }]
